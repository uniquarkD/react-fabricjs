{
  "name": "react-fabricjs",
  "version": "0.1.2",
  "description": "fabricjs implemented by react",
  "main": "dist/react-fabric.min.js",
  "keywords": [
    "canvas",
    "graphic",
    "react",
    "photo-editing"
  ],
  "license": "BSD",
  "scripts": {
    "clean": "rimraf dist",
    "build:uncompressed": "cross-env NODE_ENV=production webpack --config webpack.config.build.js",
