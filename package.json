    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react": "^15.6",
    "react-dom": "^15.6.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
